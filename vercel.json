{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "methods": [
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Authorization"
            },
            "continue": true
        },
        {
            "src": "/api/login",
            "dest": "/api/login.py",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/quiz",
            "dest": "/api/quiz.py",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/submit",
            "dest": "/api/submit.py",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/init-db",
            "dest": "/api/init-db.py",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}